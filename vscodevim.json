{
  "files.insertFinalNewline": true,
  "vim.incsearch": true,
  "vim.leader": " ",
  "vim.smartRelativeLine": true,
  "vim.sneak": true,
  "vim.sneakReplacesF": true,
  "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.normalModeKeyBindings": [
    {
      "before": ["C-k"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["C-j"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "1"],
      "commands": ["workbench.action.openEditorAtIndex1"]
    },
    {
      "before": ["<leader>", "2"],
      "commands": ["workbench.action.openEditorAtIndex2"]
    },
    {
      "before": ["<leader>", "3"],
      "commands": ["workbench.action.openEditorAtIndex3"]
    },
    {
      "before": ["<leader>", "4"],
      "commands": ["workbench.action.openEditorAtIndex4"]
    },
    {
      "before": ["<leader>", "5"],
      "commands": ["workbench.action.openEditorAtIndex5"]
    },
    {
      "before": ["<leader>", "6"],
      "commands": ["workbench.action.openEditorAtIndex6"]
    },
    {
      "before": ["<leader>", "7"],
      "commands": ["workbench.action.openEditorAtIndex7"]
    },
    {
      "before": ["<leader>", "8"],
      "commands": ["workbench.action.openEditorAtIndex8"]
    },
    {
      "before": ["<leader>", "9"],
      "commands": ["workbench.action.openEditorAtIndex9"]
    },
    {
      "before": ["<leader>", "-"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<leader>", "+"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["[", "b"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["]", "b"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["[", "t"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["]", "t"],
      "commands": ["workbench.action.nextEditor"]
    }
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["C-a"],
      "commands": ["cursorHome"]
    },
    {
      "before": ["C-e"],
      "commands": ["cursorEnd"]
    },
    {
      "before": ["C-b"],
      "after": ["<Left>"]
    },
    {
      "before": ["C-f"],
      "after": ["<Right>"]
    },
    {
      "before": ["C-d"],
      "after": ["<Del>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", ":"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "="],
      "commands": [
        {
          "command": "editor.action.formatDocument",
          "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
        },
        {
          "command": "notebook.format",
          "when": "notebookEditable && !editorTextFocus && activeEditor == 'workbench.editor.notebook'"
        }
      ]
    },
    {
      "before": ["<leader>", "b", "b"],
      "commands": ["workbench.files.action.focusOpenEditorsView"]
    },
    {
      "before": ["<leader>", "b", "d"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "b", "h"],
      "commands": ["gitlens.showQuickFileHistory"]
    },
    {
      "before": ["<leader>", "b", "w"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "c", "<space>"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["<leader>", "c", "c"],
      "commands": ["editor.action.addCommentLine"]
    },
    {
      "before": ["<leader>", "c", "u"],
      "commands": ["editor.action.removeCommentLine"]
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "f", "s"],
      "commands": ["workbench.view.scm"]
    },
    {
      "before": ["<leader>", "g", "d"],
      "commands": ["git.openChange"]
    },
    {
      "before": ["<leader>", "g", "f"],
      "commands": ["git.fetch"]
    },
    {
      "before": ["<leader>", "g", "g"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "g", "h"],
      "commands": ["gitlens.showQuickRepoHistory"]
    },
    {
      "before": ["<leader>", "g", "l"],
      "commands": ["git.pull"]
    },
    {
      "before": ["<leader>", "g", "m"],
      "commands": ["git.merge"]
    },
    {
      "before": ["<leader>", "g", "p"],
      "commands": ["git.push"]
    },
    {
      "before": ["<leader>", "g", "s"],
      "commands": ["workbench.view.scm"]
    },
    {
      "before": ["<leader>", "j", "d"],
      "commands": ["editor.action.goToDeclaration"]
    },
    {
      "before": ["<leader>", "j", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["<leader>", "j", "j"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["<leader>", "j", "r"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["<leader>", "j", "t"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["<leader>", "n", "j"],
      "commands": ["revealInExplorer"]
    },
    {
      "before": ["<leader>", "n", "n"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "s", "a"],
      "commands": ["workbench.view.search"]
    },
    {
      "before": ["<leader>", "s", "g"],
      "commands": ["workbench.view.search"]
    },
    {
      "before": ["<leader>", "s", "r"],
      "commands": ["workbench.view.search"]
    },
    {
      "before": ["<leader>", "t", "n"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "y", "f"],
      "commands": ["copyFilePath"]
    },
    {
      "before": ["<leader>", "y", "n"],
      "commands": ["copyRelativeFilePath"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "="],
      "commands": ["editor.action.formatSelection"]
    },
    {
      "before": ["<leader>", "c", "<space>"],
      "commands": ["editor.action.blockComment"]
    },
    {
      "before": ["<leader>", "c", "c"],
      "commands": ["editor.action.addCommentLine"]
    },
    {
      "before": ["<leader>", "c", "u"],
      "commands": ["editor.action.removeCommentLine"]
    }
  ]
}
